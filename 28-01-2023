//search in a binary search tree

class Solution {
    public TreeNode searchBST(TreeNode root, int val) {
        //TreeNode res=null;
        if(root==null){
            return null;
        }
        if(root.val==val){
            return root;
        }
        if(val<root.val){
            return searchBST(root.left,val);
        }
        return searchBST(root.right,val);
    }
}
